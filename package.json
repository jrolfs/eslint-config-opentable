{
  "name": "@jrolfs/eslint-config-opentable",
  "version": "7.0.0",
  "description": "This package provides OpenTable's .eslintrc as an extensible shared config.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "tests-only": "babel-tape-runner ./test/*.js",
    "travis": "npm run --silent test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opentable/eslint-config-opentable.git"
  },
  "keywords": [
    "eslint",
    "opentable",
    "config"
  ],
  "author": "Timmy Willison <timmywillisn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opentable/eslint-config-opentable/issues"
  },
  "homepage": "https://github.com/opentable/eslint-config-opentable",
  "dependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^4.9.0",
    "outdent": "^0.4.0-rc2",
    "tape": "^4.8.0"
  }
}
